<% @services.each do |service| %>
    <h3><%= service.name %> <%= service.floor %></h3>

    <% @sectors.each do |sector| %>
        <h5><%= sector.name %></h5>

        <% @rooms.each do |room| %>
            <h6><%= room.name %></h6>

            <table class="table">
                <tr>
                    <th></th>
                <tr>
                <% @ips[room].each do |ip| %>
                <% end %>
            </table>
        <% end %>
    <% end %>
<% end %>